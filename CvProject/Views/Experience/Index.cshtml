@using CvProject.Models.Entities
@model List<Experience>
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<br /><br />
<h2>Deneyim Listesi</h2>
<br />
<table class="table table-bordered">
    <tr>
        <th>ID</th>
        <th>BAŞLIK</th>
        <th>ALT BAŞLIK</th>
        <th>TARİH</th>
        <th>SİL</th>
        <th>DÜZENLE</th>
    </tr>
    @foreach (var item in Model)
    {
    <tr>
        <td>@item.Id</td>
        <td>@item.Title</td>
        <td>@item.Subtitle</td>
        <td>@item.Date</td>
        <td><a href="/Experience/Delete/@item.Id" class="btn btn-danger">Sil</a></td>
        <td><a href="/Experience/Update/@item.Id" class="btn btn-warning">Düzenle</a></td>
    </tr>
    }
    
</table>
<a href="/Experience/Add/" class="btn btn-primary">Yeni Deneyim Ekle</a>


