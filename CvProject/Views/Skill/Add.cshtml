@model CvProject.Models.Entities.Skill
@{
    ViewData["Title"] = "Add";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<br />
<form method="post" class="col md-12">
    <h2>Yeni Yetenek Ekleme</h2>
    <br />
    @Html.Label("Yetenek")
    @Html.TextBoxFor(x => x.Skill1, new { @class = "form-control" })
    <br />
    @Html.Label("Oran")
    @Html.TextBoxFor(x => x.Oran, new { @class = "form-control" })
    <br />
    <button class="btn btn-info ">Kaydet</button>
</form>






